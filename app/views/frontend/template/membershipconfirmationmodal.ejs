<div class="modal fade" id="ConfirmModel">
<div class="modal-dialog modal-lg" role="document">
    <div class="revstance_modal modal-content">
      <div class="modal-body">
        <div class="container">
          <div class="check_close">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="row">
            <form method="post" action="/category/store" name="categoryform" id="categoryform">
              <input type="hidden" name="propertyId" id="propId" value="">                  
              <div class="review-title">
                <h2 class="edit-title text-left">Upgrade Plan</h2>
              </div>
              <div class="feeling-happy">
                <div class="upgrade_plan">
                  <p class="upgrade-msg"><%=user.membership.membership_title%></p>
                </div>
              </div>
              <div class="post-review">
                <button  type="button" class="submit-category_plan confirm-plan" id="confirmPlan">Yes</button>
                <button  type="button" class="submit-category_plan" data-dismiss="modal">No</button>
              </div>
            </form>
        </div>
      </div>
    </div>
  </div>
</div>
  